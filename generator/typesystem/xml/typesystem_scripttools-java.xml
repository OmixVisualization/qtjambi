<typesystem package="io.qt.script.tools" module="qtjambi.scripttools" default-superclass="io.qt.QtObject">
    <required-library name="QtWidgets"/>
    <required-library name="QtScript"/>
    <object-type name="QScriptEngineDebugger">
        <modify-function signature="standardWindow() const">
            <modify-argument index="0">
                <conversion-rule class="native">
                    jobject %out = qtjambi_from_qobject(%env, %in, "io/qt/widgets/QMainWindow");
                </conversion-rule>
            </modify-argument>
        </modify-function>
        <modify-function signature="createStandardToolBar(QWidget *)">
            <modify-argument index="0">
                <conversion-rule class="native">
                    jobject %out = qtjambi_from_qobject(%env, %in, "io/qt/widgets/QToolBar");
                </conversion-rule>
            </modify-argument>
        </modify-function>
        <modify-function signature="createStandardMenu(QWidget *)">
            <modify-argument index="0">
                <conversion-rule class="native">
                    jobject %out = qtjambi_from_qobject(%env, %in, "io/qt/widgets/QMenu");
                </conversion-rule>
            </modify-argument>
        </modify-function>
        <modify-function signature="widget(QScriptEngineDebugger::DebuggerWidget) const">
            <modify-argument index="0">
                <conversion-rule class="native">
                    jobject %out = qtjambi_from_qobject(%env, %in, "io/qt/widgets/QWidget");
                </conversion-rule>
            </modify-argument>
        </modify-function>
        <modify-function signature="action(QScriptEngineDebugger::DebuggerAction) const">
            <modify-argument index="0">
                <conversion-rule class="native">
                    jobject %out = qtjambi_from_qobject(%env, %in, "io/qt/widgets/QAction");
                </conversion-rule>
            </modify-argument>
        </modify-function>
    </object-type>
    <suppress-warning text="WARNING(CppImplGenerator) :: protected function '*' in final class '*'" />
    <suppress-warning text="WARNING(MetaJavaBuilder) :: visibility of function '*' modified in class '*'" />
    <suppress-warning text="WARNING(MetaJavaBuilder) :: hiding of function '*' in class '*'" />
</typesystem>
